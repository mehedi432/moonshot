#: venv/lib/python3.8/site-packages/django/contrib/postgres/apps.py:49
msgid "PostgreSQL extensions"
msgstr "PostgreSQL-Erweiterungen"

#: venv/lib/python3.8/site-packages/django/contrib/postgres/fields/array.py:21
#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/array.py:17
#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/array.py:185
#, python-format
msgid "Item %(nth)s in the array did not validate:"
msgstr "Element %(nth)s im Array konnte nicht validiert werden:"

#: venv/lib/python3.8/site-packages/django/contrib/postgres/fields/array.py:22
msgid "Nested arrays must have the same length."
msgstr "Verschachtelte Arrays müssen die gleiche Länge haben."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/fields/hstore.py:15
msgid "Map of strings to strings/nulls"
msgstr "Zuordnung von Zeichenketten zu Zeichenketten/NULLs"

#: venv/lib/python3.8/site-packages/django/contrib/postgres/fields/hstore.py:17
#, python-format
msgid "The value of “%(key)s” is not a string or null."
msgstr "Der Wert für „%(key)s“ ist keine Zeichenkette oder NULL."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/hstore.py:17
msgid "Could not load JSON data."
msgstr "Konnte JSON-Daten nicht laden."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/hstore.py:18
msgid "Input must be a JSON dictionary."
msgstr "Eingabe muss ein JSON-Dictionary sein."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:39
msgid "Enter two valid values."
msgstr "Bitte zwei gültige Werte eingeben."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:41
msgid "The start of the range must not exceed the end of the range."
msgstr "Der Anfang des Wertbereichs darf nicht das Ende überschreiten."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:96
msgid "Enter two whole numbers."
msgstr "Bitte zwei ganze Zahlen eingeben."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:102
msgid "Enter two numbers."
msgstr "Bitte zwei Zahlen eingeben."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:108
msgid "Enter two valid date/times."
msgstr "Bitte zwei gültige Datum/Uhrzeit-Werte eingeben."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:114
msgid "Enter two valid dates."
msgstr "Bitte zwei gültige Kalenderdaten eingeben."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/validators.py:15
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"Liste enthält %(show_value)d Element, es sollte aber nicht mehr als "
"%(limit_value)d enthalten."
msgstr[1] ""
"Liste enthält %(show_value)d Elemente, es sollte aber nicht mehr als "
"%(limit_value)d enthalten."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/validators.py:25
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"Liste enthält %(show_value)d Element, es sollte aber nicht weniger als "
"%(limit_value)d enthalten."
msgstr[1] ""
"Liste enthält %(show_value)d Elemente, es sollte aber nicht weniger als "
"%(limit_value)d enthalten."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/validators.py:38
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "Einige Werte fehlen: %(keys)s"

#: venv/lib/python3.8/site-packages/django/contrib/postgres/validators.py:39
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "Einige unbekannte Werte wurden eingegeben: %(keys)s"

#: venv/lib/python3.8/site-packages/django/contrib/postgres/validators.py:81
#, python-format
msgid ""
"Ensure that this range is completely less than or equal to %(limit_value)s."
msgstr "Der Wertebereich muss kleiner oder gleich zu %(limit_value)s sein."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/validators.py:90
#, python-format
msgid ""
"Ensure that this range is completely greater than or equal to "
"%(limit_value)s."
msgstr "Der Wertebereich muss größer oder gleich zu %(limit_value)s sein."
